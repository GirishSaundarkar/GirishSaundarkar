<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration SYSTEM
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
<session-factory>
<property name="hibernate.dilect">org.hibernate.dialect.mySQLdialect</property>
<property name="hibernate.driver_class">com.mysql.Driver</property>
<property name="hibernate.connection.url">jdbc:mysql:/localhost:3306/studdb</property>
<property name="hibernate.connection.username"></property>
<property name="hibernate.connection.password"></property>
</session-factory>
</hibernate-configuration>
